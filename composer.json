{
  "name": "kmalarifi/laravel-payment-gateways",
  "description": "Flexible multi-gateway payment abstraction for Laravel 11.",
  "type": "library",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0",
    "illuminate/http": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Kmalarifi\\PaymentGateways\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kmalarifi\\PaymentGateways\\Providers\\PaymentGatewaysServiceProvider"
      ]
    }
  },
  "authors": [
    {
      "name": "Khalid Alâ€‘Arifi",
      "email": "kmalarifi97@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require-dev": {
    "pestphp/pest": "^3.8",
    "orchestra/testbench": "^9.13"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
